public class FizzBuzz {

    public static void main(String args[])
    {
        int n = 50;

        for (int i = 0; i <= n; i++)
        {
            if (i % 3 == 0 && i % 5 == 0)
                System.out.println("FizzBuzz" + " " + i);

            else if (i % 5 == 0)
                System.out.println("Buzz");

            else if (i % 3 == 0)
                System.out.println("Fizz");

            else
                System.out.println(i);
        }
    }
}
